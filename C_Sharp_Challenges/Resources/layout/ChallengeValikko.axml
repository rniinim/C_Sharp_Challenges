<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <TextView
            android:layout_marginTop="5dip"
            android:text="Difficulty: Easy"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dip"
            android:layout_marginRight="10dip"
            android:id="@+id/easyChallengetxt"
            android:textColor="@android:color/holo_green_dark" />
        <Button
            android:text="Ask Name"
            android:id="@+id/btn01"
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            android:padding="20dip"
            android:layout_marginLeft="10dip"
            android:layout_marginRight="10dip" />
        <Button
            android:id="@+id/btn11"
            android:text="Print Numbers"
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            android:padding="20dip"
            android:layout_marginLeft="10dip"
            android:layout_marginRight="10dip" />
        <Button
            android:text="Addition"
            android:id="@+id/btn21"
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            android:padding="20dip"
            android:layout_marginLeft="10dip"
            android:layout_marginRight="10dip" />
        <Button
            android:text="Even Or Odd"
            android:id="@+id/btn02"
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            android:padding="20dip"
            android:layout_marginLeft="10dip"
            android:layout_marginRight="10dip" />
        <Button
            android:text="Letters Of Word"
            android:id="@+id/btn12"
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            android:padding="20dip"
            android:layout_marginLeft="10dip"
            android:layout_marginRight="10dip" />
        <Button
            android:text="Print Odd Numbers"
            android:id="@+id/btn22"
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            android:padding="20dip"
            android:layout_marginLeft="10dip"
            android:layout_marginRight="10dip" />
        <TextView
            android:text="Difficulty: Medium"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dip"
            android:layout_marginRight="10dip"
            android:layout_marginTop="5dip"
            android:id="@+id/easyChallengetxt"
            android:textColor="@android:color/holo_orange_dark" />
        <Button
            android:text="Head Or Tails"
            android:id="@+id/btn03"
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            android:padding="20dip"
            android:layout_marginLeft="10dip"
            android:layout_marginRight="10dip" />
        <Button
            android:text="Number Password Generator"
            android:id="@+id/btn13"
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            android:padding="20dip"
            android:layout_marginLeft="10dip"
            android:layout_marginRight="10dip" />
        <Button
            android:text="Years In Seconds"
            android:id="@+id/btn23"
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            android:padding="20dip"
            android:layout_marginLeft="10dip"
            android:layout_marginRight="10dip" />
        <Button
            android:text="°C to °F"
            android:id="@+id/btn04"
            android:textAlignment="center"
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            android:padding="20dip"
            android:layout_marginLeft="10dip"
            android:layout_marginRight="10dip" />
        <Button
            android:text="Russian Roulette Game"
            android:id="@+id/btn14"
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            android:padding="20dip"
            android:layout_marginLeft="10dip"
            android:layout_marginRight="10dip" />
        <Button
            android:text="Simple Calculator"
            android:id="@+id/btn24"
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            android:padding="20dip"
            android:layout_marginLeft="10dip"
            android:layout_marginRight="10dip" />
        <TextView
            android:text="Difficulty: Hard"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dip"
            android:layout_marginTop="5dip"
            android:layout_marginRight="10dip"
            android:id="@+id/easyChallengetxt"
            android:textColor="@android:color/holo_red_light" />
        <Button
            android:text="Multiplication Table"
            android:id="@+id/btn05"
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            android:padding="20dip"
            android:layout_marginLeft="10dip"
            android:layout_marginRight="10dip" />
        <Button
            android:text="Password Generator"
            android:id="@+id/btn15"
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            android:padding="20dip"
            android:layout_marginLeft="10dip"
            android:layout_marginRight="10dip" />
        <Button
            android:text="Rock - Paper - Scissors"
            android:id="@+id/btn25"
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            android:padding="20dip"
            android:layout_marginLeft="10dip"
            android:layout_marginRight="10dip" />
        <Button
            android:text="Reverse Word"
            android:id="@+id/btn06"
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            android:padding="20dip"
            android:layout_marginLeft="10dip"
            android:layout_marginRight="10dip" />
        <Button
            android:text="Decimal to Binary"
            android:id="@+id/btn16"
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            android:padding="20dip"
            android:layout_marginLeft="10dip"
            android:layout_marginRight="10dip" />
        <Button
            android:text="Volume of a Sphere"
            android:id="@+id/btn26"
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            android:padding="20dip"
            android:layout_marginLeft="10dip"
            android:layout_marginRight="10dip"
            android:layout_marginBottom="50dip" />
    </LinearLayout>
</ScrollView>